type Mutation {
    createOrganization(name:String!):Organization!
    createOrganizationAccessToken(orgId:ID!):String!
    createAlerter(inputAlerter:IAlerter!):Alerter!
    updateAlerter(alerterId:ID!,inputAlerter:IAlerter!):Alerter!
    deleteAlerter(alerterId:ID!):String!
    updateTrigger(triggerId:ID!,inputTrigger:ITrigger!):Trigger!
    deleteTrigger(triggerId:ID!):String!
}
