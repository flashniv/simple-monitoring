type Trigger{
    id:ID!
    organization:Organization!
    name:String!
    description:String!
    triggerId:String!
    lastStatus:TriggerStatus!
    priority:TriggerPriority!
    lastStatusUpdate:String!
    enabled:Boolean!
    suppressedScore:Int!
    muted:Boolean!
    conf:String!
    alerts(page:Int = 0,size:Int = 50):[Alert!]
}
